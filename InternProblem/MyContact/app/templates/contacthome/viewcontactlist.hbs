 <h1 class="text-center p-3"> List of contacts</h1>
  <table class="table container ">
    <thead class="thead-dark">
      <tr>
        
        <th scope="col">Name</th>
        <th scope="col">Number</th>
        <th scope="col">Email Id</th>
        <th scope="col">Remove</th>
        <th scope="col">Ubdate</th>
      </tr>
    </thead>
    <tbody>

        {{#each Details.contactDetails as |Detail| }}
            <tr>                
                <td>{{Detail.name}}</td>
                <td>{{Detail.number}}</td>
                <td>{{Detail.email}}</td>
               <td><button {{action "delete" Detail.id }}>Delete</button></td>
               <td><button {{action "edit" Detail.id }}>Edit</button></td>
            </tr>
     {{/each}}
    </tbody>
  </table>
    
    <div class="text-center">
           <button type="button" class="btn btn-primary ml-5 " {{action "back" }}>back</button>
    </div>
   

{{outlet}}